{
  "name": "com.xygen.qrtoken",
  "version": "1.0.0",
  "description": "API generacion de token",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "introspect": "drizzle-kit introspect:pg --config=./drizzle.config.ts",
    "migration:generate": "drizzle-kit generate:pg --schema=./models/schema.ts",
    "migration:push": "bun run ./models/migrate.ts",
    "migration": "bun run migration:generate && bun run migration:push",
    "dev": "bun run src/index.ts ",
    "lint": "bun run oxlint",
    "test": "bun run test"
  },
  "author": "Xygen.io",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "drizzle-orm": "^0.29.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "bcrypt": "^5.1.0",
    "bun-types": "^1.0.21",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.10",
    "googleapis": "^130.0.0",
    "jsonwebtoken": "^9.0.1",
    "nodemon": "^3.0.2",
    "oxlint": "^0.0.22",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  }
}
